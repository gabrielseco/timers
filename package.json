{
  "name": "timers",
  "version": "1.0.0",
  "description": "Countdown for your salary",
  "main": "index.js",
  "scripts": {
    "clean:modules": "rm -rf node_modules",
    "coverage:open": "open coverage/lcov-report/index.html",
    "precommit": "lint-staged",
    "commit": "git-cz",
    "dev": "node config/browser-open.js && node config/webpack-server.js",
    "prebuild": "rm -rf ./public",
    "build": "webpack -p --config ./config/webpack.prod.config.js",
    "postbuild": "mv public/static/index.html public/index.html",
    "serve:prod": "http-server public -p 8080",
    "test": "jest --config ./jest.config.json",
    "test:watch": "jest --watchAll --config ./jest.config.json",
    "lint:js": "eslint ./src/**/*.js --color",
    "lint:css": "stylelint 'src/**/*.scss'",
    "generate:stateful": "node ./scripts/stateful.js",
    "generate:stateless": "node ./scripts/stateless.js"
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "prettier --write --single-quote",
      "git add"
    ],
    "*.scss": "stylelint"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": "conventional-commit-types",
      "helpMessage": "Use \"npm run commit\" instead, we use conventional-changelog format :) (https://github.com/commitizen/cz-cli)"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "keywords": [],
  "author": "Gabriel Garc√≠a Seco",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.42",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.42",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0-beta.42",
    "@babel/preset-react": "^7.0.0-beta.42",
    "@babel/presetn-env": "^7.0.0-beta.42",
    "autoprefixer": "^8.1.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "babel-loader": "^8.0.0-beta.2",
    "commitizen": "^2.9.6",
    "css-loader": "^0.28.10",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "genmite": "^1.1.6",
    "html-webpack-plugin": "^3.0.6",
    "http-server": "^0.11.1",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "jest-css-modules": "^1.1.0",
    "lint-staged": "^7.0.0",
    "mini-css-extract-plugin": "^0.2.0",
    "node-sass": "^4.7.2",
    "open": "0.0.5",
    "postcss-loader": "^2.1.1",
    "prettier": "^1.11.1",
    "react-hot-loader": "^4.0.0",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "stylelint": "^9.1.3",
    "stylelint-config-sass-guidelines": "^5.0.0",
    "stylelint-order": "^0.8.1",
    "stylelint-scss": "^2.5.0",
    "validate-commit-msg": "^2.14.0",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "luxon": "^0.5.8",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
